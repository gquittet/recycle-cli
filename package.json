{
  "name": "recycle-cli",
  "version": "1.0.0",
  "description": "An unofficial Recycle CLI to generate an iCal file.",
  "repository": "https://github.com/gquittet/recycle-cli",
  "license": "MIT",
  "author": {
    "email": "contact@gqode.be",
    "name": "Guillaume Qode",
    "url": "https://gqode.be"
  },
  "keywords": [
    "recycle",
    "recycle-cli",
    "ical",
    "cli",
    "gqode"
  ],
  "bin": "dist/cli.js",
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "prebuild": "rimraf dist/",
    "postbuild": "chmod +x dist/cli.js",
    "preversion": "npm run build && npm run test",
    "prepublish": "npm run build && npm run test",
    "build": "tsc",
    "dev": "tsc --watch",
    "format": "prettier --write .",
    "test": "xo && prettier --check . && xo"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "axios": "^1.13.2",
    "axios-retry": "^4.5.0",
    "ink": "^6.5.1",
    "ink-link": "^5.0.0",
    "ink-select-input": "^6.2.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "luxon": "^3.7.2",
    "meow": "^14.0.0",
    "prop-types": "^15.8.1",
    "react": "^19.2.1"
  },
  "devDependencies": {
    "@sindresorhus/tsconfig": "^8.1.0",
    "@types/luxon": "^3.7.1",
    "@types/node": "^24.10.1",
    "@types/prop-types": "^15.7.15",
    "@types/react": "^19.2.7",
    "chalk": "^5.6.2",
    "eslint-config-xo-react": "^0.29.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "ink-testing-library": "^4.0.0",
    "prettier": "^3.7.4",
    "rimraf": "^6.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "xo": "^1.2.3"
  },
  "ava": {
    "extensions": {
      "ts": "module",
      "tsx": "module"
    },
    "nodeArguments": [
      "--loader=ts-node/esm"
    ]
  }
}
